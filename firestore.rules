
rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {

    // AVISO: As regras a seguir tornam seu banco de dados público.
    // Qualquer pessoa pode ler, escrever e deletar seus dados.
    // Use esta configuração apenas para depuração e nunca em produção.
    match /{path=**} {
      allow read, write: if true;
    }
  }
}
